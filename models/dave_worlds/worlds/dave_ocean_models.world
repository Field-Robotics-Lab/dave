      <uri>https://fuel.gazebosim.org/1.0/Cole/models/Lionfish</uri>
    </include>

    <include>
      <static>true</static>
      <pose>12 -11.5 -94.8 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/Cole/models/Coral01</uri>
    </include>

    <include>
      <static>true</static>
      <pose>12.2 -10.3 -95 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/Cole/models/Coral02</uri>
    </include>

    <include>
      <static>true</static>
