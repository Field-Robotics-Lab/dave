      <uri>model://niskin</uri>
    </include>

    <include>
      <static>true</static>
      <pose>12 -10 -94.8 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/Cole/models/Kelp 01</uri>
    </include>

    <include>
      <static>true</static>
