    </include>

    <include>
      <name>hardhat_standard</name>
      <pose>13 0 -94 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/quarkytale/models/Hardhat Standard</uri>
    </include>

     <include>
      <name>hardhat_ribbed</name>
      <pose>13 1 -94 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/quarkytale/models/Hardhat Ribbed</uri>
    </include>

    <include>
      <name>hardhat_superribbed</name>
      <pose>13 2 -94 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/quarkytale/models/Hardhat Superribbed</uri>
    </include>

    <include>
      <name>hardhat_octagonal</name>
      <pose>13 3 -94 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/quarkytale/models/Hardhat Octagonal</uri>
    </include>

    <!-- Still pending
    <include>
      <name>airplane_wreck</name>
      <pose>20 -12 -94 0 0 0</pose>
      <uri>model://airplane_wreck</uri>
    </include>
    -->

    <include>
      <name>mbari_mars</name>
      <pose>13 6 -94 0 0 0</pose>
      <uri>https://fuel.gazebosim.org/1.0/quarkytale/models/MBARI MARS</uri>
    </include>

    <include>
      <pose>13 -7 -94.5 0 0 0</pose>
      <!--uri>https://fuel.gazebosim.org/1.0/Cole/models/Sunken Vase</uri-->
